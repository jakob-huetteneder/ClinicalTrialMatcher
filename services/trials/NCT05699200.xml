  <primary_outcome>
    <measure>Arterial thickness</measure>
    <time_frame>Baseline</time_frame>
    <description>Measured by carotid ultrasound intimal media thickness reporting in mm</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Arterial stiffness</measure>
    <time_frame>Baseline</time_frame>
    <description>Measured by carotid ultrasound pulse wave velocity reporting in m/s</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Baroreflex sensitivity</measure>
    <time_frame>Baseline</time_frame>
    <description>The slope between heart rate and blood pressure at rest and during the Valsalva Maneuver (ms/mmHg)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Sympathetic response to stress</measure>
    <time_frame>Baseline</time_frame>
    <description>Blood pressure or muscle sympathetic nerve activity response to sympathoexcitatory stressors (mm/Hg or bursts/min).</description>
  </primary_outcome>
