  <brief_title>Comparisons of the Therapeutic Effects of Dienogest and Danazol on Endometriosis</brief_title>
  <official_title>Far Eastern Memorial Hospital</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Far Eastern Memorial Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
